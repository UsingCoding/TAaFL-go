<Program> -> <main> <openPar> <closePar> <statements>
<main> -> mainToken
<openPar> -> openParenthesis
<closePar> -> closingParenthesis
<statements> -> <var>
<var> -> <intToken> <id> <appropriation> <integer> <separator>
<intToken> -> intToken
<id> -> id
<appropriation> -> appropriation
<integer> -> integer
<separator> -> separator